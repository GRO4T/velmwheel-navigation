<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">

    <!-- Package's name -->
    <name>lms1xx</name>
    <!-- Package's version -->
    <version>1.0.0</version>
    <!-- Package's description -->
    <description>
    
    Refreshed implementation of the lms1xx package providing C++ library for managing LIDAR sensors of the LMS1xx family
    (see original package at <a href="https://github.com/dudekw/LMS1xx/">ros wiki</a>)

    </description>
    <!-- Package's author -->
    <author>Konrad Banachowicz</author>
    <author email="mpurvis@clearpathrobotics.com">Mike Purvis</author>
    <author>Wojtek Dudek</author>
    <!-- Package's maintainer -->
    <maintainer email="krzysztof.pierczyk@gmail.com">Krzysiek Pierczyk</maintainer>
    <!-- Package's license -->
    <license>MIT</license>
    
    <!-- Build tool dependencies -->
    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>cmake_common</buildtool_depend>
    <buildtool_depend>doc_common</buildtool_depend>
    <!-- Build dependencies -->
    <build_depend>rclcpp</build_depend>
    <!-- Runtime dependencies -->
    <exec_depend>rclcpp</exec_depend>

    <!-- Package's exports -->
    <export>
      <build_type>ament_cmake</build_type>
    </export>

</package>
