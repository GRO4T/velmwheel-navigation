<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">

    <!-- Package's name -->
    <name>velmwheel_bringup</name>
    <!-- Package's version -->
    <version>0.0.0</version>
    <!-- Package's description -->
    <description>Set of ROS2 launch files for the WUT velmobil robot launching simulation</description>
    <!-- Package's author -->
    <author email="krzysztof.pierczyk@gmail.com">Krzysztof Pierczyk</author>
    <!-- Package's maintainer -->
    <maintainer email="krzysztof.pierczyk@gmail.com">Krzysztof Pierczyk</maintainer>
    <!-- Package's license -->
    <license>Apache License 2.0</license>

    <!-- Package's build dependencies -->
    <buildtool_depend>ament_cmake</buildtool_depend>
    <!-- Package's dependencies -->
    <depend>velmwheel_launch</depend>
    <depend>velmwheel_sim_bringup</depend>
    <!-- <depend>velmwheel_drivers_bringup</depend> -->
    <depend>velmwheel_middleware_bringup</depend>
    <!-- Package's runtime dependencies -->
    <exec_depend>ros2launch</exec_depend>
    <exec_depend>launch_common</exec_depend>
    <exec_depend>teleop_twist_keyboard</exec_depend>

    <!-- Package's exports -->
    <export>
        <build_type>ament_cmake</build_type>
    </export>

</package>
