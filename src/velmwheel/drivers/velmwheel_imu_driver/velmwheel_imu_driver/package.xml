<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">

    <!-- Package's name -->
    <name>velmwheel_imu_driver</name>
    <!-- Package's version -->
    <version>0.0.0</version>
    <!-- Package's description -->
    <description>Driver plugin for the IMU EtherCAT slave</description>
    <!-- Package's maintainer -->
    <maintainer email="krzysztof.pierczyk@gmail.com">Krzysztof Pierczyk</maintainer>
    <!-- Package's license -->
    <license>Apache License 2.0</license>

    <!-- Package's build dependencies -->
    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>ament_cmake_python</buildtool_depend>
    <buildtool_depend>cmake_common</buildtool_depend>
    <!-- Package's buildtime dependencies -->
    <build_depend>rclcpp</build_depend>
    <build_depend>pluginlib</build_depend>
    <build_depend>node_common_cpp</build_depend>
    <build_depend>imu_ethercat</build_depend>
    <build_depend>velmwheel_ethercat_driver</build_depend>
    <build_depend>sensor_msgs</build_depend>
    <build_depend>velmwheel_imu_driver_msgs</build_depend>
    <!-- Package's runtime dependencies -->
    <exec_depend>rclcpp</exec_depend>
    <exec_depend>pluginlib</exec_depend>
    <exec_depend>node_common_cpp</exec_depend>
    <exec_depend>sensor_msgs</exec_depend>
    <exec_depend>velmwheel_imu_driver_msgs</exec_depend>

    <!-- Package's exports -->
    <export>
        <build_type>ament_cmake</build_type>
    </export>

</package>
