<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">

    <!-- Package's name -->
    <name>velmwheel_global_localization</name>
    <!-- Package's version -->
    <version>0.0.5</version>
    <!-- Package's description -->
    <description>

    Package providing a global localization node for the WUT Velmwheel robot based on matching
    of characteristic points from the robot's environment

    </description>
    <!-- Package's author -->
    <author email="wojciechdudek.mail@gmail.com">Wojciech Dudek</author>
    <!-- Package's maintainer -->
    <maintainer email="krzysztof.pierczyk@gmail.com">Krzysztof Pierczyk</maintainer>
    <!-- Package's license -->
    <license>BSD</license>

    <!-- Package's build dependencies -->
    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>cmake_common</buildtool_depend>
    <!-- Package's buildtime dependencies -->
    <build_depend>rclcpp_components</build_depend>
    <build_depend>node_common_cpp</build_depend>
    <build_depend>sensor_msgs</build_depend>
    <build_depend>geometry_msgs</build_depend>
    <build_depend>nav_msgs</build_depend>
    <build_depend>visualization_msgs</build_depend>
    <build_depend>poi_map_msgs</build_depend>
    <build_depend>tf2_ros</build_depend>
    <build_depend>tf2_eigen</build_depend>
    <build_depend>tf2_geometry_msgs</build_depend>
    <build_depend>velmwheel_global_localization_msgs</build_depend>
    <build_depend>velmwheel_common</build_depend>
    <build_depend>poi_map_lib</build_depend>
    <build_depend>eigen3_cmake_module</build_depend>
    <build_depend>libpcl-all-dev</build_depend>
    <!-- Package's runtime dependencies -->
    <exec_depend>rclcpp_components</exec_depend>
    <exec_depend>sensor_msgs</exec_depend>
    <exec_depend>geometry_msgs</exec_depend>
    <exec_depend>nav_msgs</exec_depend>
    <exec_depend>visualization_msgs</exec_depend>
    <exec_depend>poi_map_msgs</exec_depend>
    <exec_depend>tf2_ros</exec_depend>
    <exec_depend>tf2_eigen</exec_depend>
    <exec_depend>tf2_geometry_msgs</exec_depend>
    <exec_depend>velmwheel_global_localization_msgs</exec_depend>
    <exec_depend>velmwheel_common</exec_depend>
    <exec_depend>poi_ma_libp</exec_depend>
    <exec_depend>eigen3_cmake_module</exec_depend>
    <exec_depend>libpcl-all-dev</exec_depend>
    <!-- Package's runtime dependencies (launch) -->
    <exec_depend>velmwheel_launch</exec_depend>

    <!-- Package's exports -->
    <export>
        <build_type>ament_cmake</build_type>
    </export>

</package>